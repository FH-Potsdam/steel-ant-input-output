{
  "name": "p5js-starter-kit",
  "version": "1.0.0",
  "description": "getting started",
  "main": "sketch.js",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.1.2",
    "nib": "latest",
    "stylus": "latest"
  },
  "scripts": {
    "start": "npm run build",
    "build:copy": "cp ./src/index.html ./public/index.html",
    "build:browserify": "babel ./src/sketch.js -o ./public/sketch.js",
    "build:stylus": "stylus -u nib ./src/main.styl > ./public/main.css",
    "build:css": "npm run build:stylus",
    "build": "npm run build:copy && npm run build:css && npm run build:browserify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "p5"
  ],
  "author": "fabiantheblind",
  "license": "ISC"
}
